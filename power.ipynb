    "class Power():\n",
    "    def __init__(self,x,n):\n",
    "        self.X=x\n",
    "        self.N=n\n",
    "    def print_pow(self):\n",
    "        y=self.X**self.N\n",
    "        print('n-th power of x is:',y)\n",
    "P=Power(int(input('enter value of x:')),int(input('enter value of n:')))\n",
    "P.print_pow()"
