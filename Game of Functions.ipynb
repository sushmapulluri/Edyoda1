    "def sum_of_numbers(s):    \n",
    "    total = 0                       \n",
    "    for num in s:      \n",
    "        total+= num\n",
    "    return total\n",
    "s = [8, 2, 3, 0, 7]      \n",
    "sum = sum_of_numbers(s)\n",
    "print(f\" sum of number {s} = {sum}\")"
